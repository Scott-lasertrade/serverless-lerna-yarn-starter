{
    "extends": "../tsconfig.services.json",
    "compilerOptions": {
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "outDir": "lib",
        "baseUrl": ".",
        "target": "ES2020"
    },
    "include": ["./**/*.ts"],
    "exclude": [
        "node_modules/**/*",
        ".serverless/**/*",
        ".webpack/**/*",
        ".build/**/*",
        "**/__tests__/*",
        "**/__e2e__/*"
    ]
}
